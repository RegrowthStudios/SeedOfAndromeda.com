    <option option_id="rawhypertext1_css" edit_format="textbox" data_type="string" display_order="2" validation_class="EWRporta_Block_Option_RawHyperText1" validation_method="verifyCSS">
      <option_value>#charIcons { text-align: center; margin-bottom: 10px; }
#charIcons ul { text-align: center; }
#charIcons ul li { display: inline-block; }
#charIcons ul li img { position: relative; z-index: 1; width: 46px; height: 46px; }

#charIcons ul li img:hover
{
	z-index: 2;
	-moz-transform:		scale(2.0);
	-webkit-transform:	scale(2.0);
	-o-transform:		scale(2.0);
	-moz-transition:		all 0.3s ease-in-out 0s;
	-o-transition:		all 0.3s ease-in-out 0s;
	-webkit-transition:	all 0.3s ease-in-out 0s;
}</option_value>
